{
  "compilerOptions": {
    "target": "es6",
    "module": "es6",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "strict": true,
    "allowJs": true,
    "isolatedModules": true,
    "noEmit": true,
    "declaration": false,
    "noImplicitOverride": true,
    "sourceMap": true,
  }
}
